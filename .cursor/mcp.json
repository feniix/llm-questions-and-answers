{
  "mcpServers": {
    "web-search": {
      "type": "stdio",
      "command": "bunx",
      "args": [
        "-y",
        "@dannyboy2042/freebird-mcp@1.5.1"
      ]
    },
    "sequential": {
      "type": "stdio",
      "command": "bunx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking@2025.7.1"
      ]
    },
    "playwright": {
      "type": "stdio",
      "command": "bunx",
      "args": [
        "-y",
        "@playwright/mcp@0.0.37"
      ]
    },
    "context7": {
      "type": "stdio",
      "command": "bunx",
      "args": [
        "-y",
        "@upstash/context7-mcp@1.0.17"
      ]
    },
    "terraform": {
      "type": "stdio",
      "command": "terraform-mcp-server",
      "args": [
        "stdio"
      ]
    },
    "basic-memory": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "basic-memory",
        "mcp"
      ]
    }
  }
}
